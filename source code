import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class MasterDataInputProgram {
    public static void main(String[] args) {
        List<String> masterData = inputMasterData();
        displayMasterData(masterData);
    }

    private static List<String> inputMasterData() {
        Scanner scanner = new Scanner(System.in);
        List<String> masterData = new ArrayList<>();
        System.out.println("Enter master data (type 'END' to exit):");
        String input;
        while (!(input = scanner.nextLine()).equals("END")) {
            masterData.add(input);
        }
        return masterData;
    }

    private static void displayMasterData(List<String> masterData) {
        System.out.println("Master data:");
        for (String data : masterData) {
            System.out.println(data);
        }
    }
}

class exception01 extends Exception {
    public exception01(String message) {
        super(message);
    }
}

public class MyClass {
    public static void main(String[] args) {
        try {
            throw new exception01("Error. Please contact administrator");
        } catch (exception01 e) {
            System.out.println(e.getMessage());
        }
    }
}
